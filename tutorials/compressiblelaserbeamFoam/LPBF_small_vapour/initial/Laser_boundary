FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    back
    {
        type            fixedValue;
        value           uniform -1;
    }
    front
    {
        type            fixedValue;
        value           uniform -1;
    }
    leftWall
    {
        type            fixedValue;
        value           uniform -1;
    }
    rightWall
    {
        type            fixedValue;
        value           uniform -1;
    }
    topWall
    {
        type            fixedValue;
        value           uniform 1;
    }
    bottomWall
    {
        type            fixedValue;
        value           uniform -1;
    }
}




