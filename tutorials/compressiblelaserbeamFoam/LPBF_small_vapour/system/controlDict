FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     compressibleLaserbeamFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         100e-6;

deltaT          1e-12;

writeControl    adjustableRunTime;

writeInterval    1e-7;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.05;//0.1;

maxAlphaCo      0.05;

maxDeltaT       1e-10;


